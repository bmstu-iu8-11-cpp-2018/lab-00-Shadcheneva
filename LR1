#include "stdafx.h"
#include<iostream>
int main()
{
	//1
    std::cout << "1" << " " << "13" << " " << "49" << " ";
	//2
	std::cout << "enter a symbol";
	char i;
    std::cin >> i;
	std::cout << "1" << i << "13" << i << "49";
	system("pause");
	//3
	int a=0;
	int b=0;
	int c=0;
	std::cout << "enter three numbers";
	std::cin >> a >> b >> c;
	std::cout << "a" << " " << "b" << " " << "c";
}
